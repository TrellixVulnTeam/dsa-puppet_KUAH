<%=
  url        = [scope.lookupvar('url')  ].flatten
  suite      = [scope.lookupvar('suite')].flatten
  components = [scope.lookupvar('components')].flatten.join(' ')

  lines = []
  url.each do |u|
    suite.each do |s|
      lines << "deb	#{u}	#{s}	#{components}"
    end
  end
  lines.join("\n")
%>
