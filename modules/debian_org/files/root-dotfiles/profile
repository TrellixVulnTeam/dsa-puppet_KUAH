# ~/.profile: executed by Bourne-compatible login shells.

## THIS FILE IS UNDER PUPPET CONTROL.
## LOCAL CHANGES WILL BE OVERWRITTEN.

if [ "$BASH" ]; then
  if [ -f ~/.bashrc ]; then
    . ~/.bashrc
  fi
  if [ "$PS1" ]; then
    PS1='${debian_chroot:+[$debian_chroot] }\h:\w\$ '
  fi
fi

mesg n

