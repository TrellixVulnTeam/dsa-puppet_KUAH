<%
	output = @rule
	if @description.length > 0
		output += ' mod comment comment "'
		output += @description
		output += '"'
	end
	unless @notarule
		output += ';'
	end
-%>
domain <%= @domain %> {
	table <%= @table %> {
		chain <%= @chain %> {
			<%= output -%>
		}
	}
}
